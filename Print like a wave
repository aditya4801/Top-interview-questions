col wise first up to down then down to up printing of numbers

vector<int> wavePrint(vector<vector<int>> arr, int nRows, int mCols)
{
vector<int>ans;

for(int col=0; col<mCols; col++{
  if(col&1){ //if col number is odd then bottom to up
    for(int row= nRows-1; row>=0; row++){
    
    ans.push_back(arr[row][col]);
    }
  }
  else{ if col is even up to bottom
    for(int row=0; row< nRows; row++){
       ans.push_back(arr[row],arr[col]);
    }
  }
 }
 return ans;
}
